{
  "name": "package-host",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:lib",
    "build:lib": "ng build ngx-progressive-image-loader && npm run copy-scss && npm run copy-readme",
    "build:lib:w": "ng build ngx-progressive-image-loader --watch",
    "publish:lib": "npm publish ./dist/ngx-progressive-image-loader",
    "copy-scss": "cpx \"./projects/ngx-progressive-image-loader/src/lib/*.scss\" \"./dist/ngx-progressive-image-loader/\" --verbose",
    "copy-readme": "cpx \"./README.md\" \"./dist/ngx-progressive-image-loader\"",
    "test": "ng test ngx-progressive-image-loader",
    "lint": "ng lint ngx-progressive-image-loader",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.6",
    "@angular/common": "^6.1.6",
    "@angular/compiler": "^6.1.6",
    "@angular/core": "^6.1.6",
    "@angular/forms": "^6.1.6",
    "@angular/http": "^6.1.6",
    "@angular/platform-browser": "^6.1.6",
    "@angular/platform-browser-dynamic": "^6.1.6",
    "@angular/router": "^6.1.6",
    "ngx-window-token": "^1.0.0",
    "core-js": "^2.5.7",
    "rxjs": "^6.3.1",
    "zone.js": "~0.8.26",
    "ngx-progressive-image-loader": "^2.0.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0-rc.0",
    "@angular-devkit/build-ng-packagr": "~0.8.0-rc.0",
    "@angular/cli": "~6.2.0-rc.0",
    "@angular/compiler-cli": "^6.1.6",
    "@angular/language-service": "^6.1.6",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.4.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.2",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.1.1",
    "protractor": "~5.4.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.3",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2",
    "cpx": "^1.5.0"
  }
}
